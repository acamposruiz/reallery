.home {
  &.container {
    position: absolute;
    & .menu.menu-content {
      & .project-button-container {
        width: 25%;
        display: flex;
        flex-direction: column;
        justify-content: center;
        & .text {
          padding: 0.5vh 0vh;
          &.type {
            display: none;
          }
        }
        & .project-icon {
          padding: 23px;
          border-radius: 8px;
        }
      }
    }
  }
}
