  "scripts": {
    "watch:generator": "watch 'npm run create:dev && npm run webpack:once' ./generator --interval=60",
    "create": "node ./generator/index",
    "create:dev": "node ./generator/index root-path=tests/e2e/mocks/site",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:open:debug": "DEBUG=cypress:* ./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run --browser chrome",
    "build": "webpack --config internals/bundleConf/webpack.config.pro.js",
    "build:tests": "npm run build && npm run test",
    "build:dev": "webpack --config internals/bundleConf/webpack.config.dev.js",
    "build:docs": "webpack --config internals/bundleConf/webpack.config.docs.js",
    "start:dev": "webpack-dev-server --config internals/bundleConf/webpack.config.dev.js",
    "start:pro": "webpack-dev-server --config internals/bundleConf/webpack.config.pro.js",
    "test": "npm run test:unit && npm run test:e2e:server",
    "test:unit": "jest",
    "test:e2e": "npm run cypress:run",
    "test:e2e:open": "npm run cypress:open",
    "test:server": "npx http-server -s -p 8989 -c1 ./build &",
    "test:e2e:server": "npm run test:server && npm run test:e2e"
  },
  "test": "jest",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.es6$": "babel-jest",
      "^.+\\.jsx$": "babel-jest",
